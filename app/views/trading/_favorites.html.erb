<table class="table" style="afont-size:12px">
  <thead>
    <tr>
      <th style="width: 85%" scope="col">Symbol</th>
      <th style="width: 15%" ></th>
    </tr>
  </thead>
  <tbody>
    <% traderStocks.each do |ts| %>
      <tr>
        <td >
          <%= content_tag "div", id: "symbolDataDivFavorites_FavoritesList", data: {id: ts.stock_symbol.id, symbol: ts.stock_symbol.symbol} do %>
          <% end %>
          <%= link_to ts.stock_symbol.symbol, trading_index_path(:symbol => ts.stock_symbol.symbol)%>
          <div style="font-size: 10px"><%=ts.stock_symbol.name%></div>
        </td>
        <td><a id="removeFromFavorites_FavoritesList" href="#"><i class="fa-solid fa-xmark"></i></a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
