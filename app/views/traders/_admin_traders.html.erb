<p style="color: green"><%= notice %></p>

<h1>Traders!!</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Balance</th>
    </tr>
  </thead>
  <tbody>   
    <% @administrator.traders.each do |trader| %>

      <%= render trader %>    
 
    <% end %>   
  </tbody>
</table>
 

<%= link_to "New trader", new_administrator_trader_path(@administrator) %>
